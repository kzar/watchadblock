{
  "name": "AdBlock",
  "version": "1.4.29",
  "description": "__MSG_description__",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "http://*/*", 
    "https://*/*"
  ],
  "background_page": "background.html",
  "options_page": "options/index.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "jquery/jquery-1.4.2.min.js", 
        "functions.js", 
        "broadcast_channel.js", 
        "adblock_start.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "overlay.js", 
        "clickwatcher.js",
        "elementchain.js",
        "blacklistui.js",
        "blacklister.js",
        "whitelister.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [ "adblock.js", "filteroptions.js" ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "default_locale": "en"
}
