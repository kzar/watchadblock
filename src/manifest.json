{
  "name": "AdBlock",
  "version": "1.4.35",
  "description": "AdBlock for Chrome!  Blocks ads all over the web.  Rated 4.5 out of 5 stars by our users!",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "http://*/*", "https://*/*"
  ],
  "background_page": "background.html",
  "options_page": "options/index.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "port.js",
        "jquery/jquery-1.4.2.min.js", 
        "functions.js", 
        "broadcast_channel.js", 
        "adblock_start.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "blacklist_wizard/overlay.js", 
        "blacklist_wizard/clickwatcher.js",
        "blacklist_wizard/elementchain.js",
        "blacklist_wizard/blacklistui.js",
        "blacklister.js", 
        "whitelister.js", 
        "filteroptions.js",
        "adblock.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
