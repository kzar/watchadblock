{
  "name": "AdBlock",
  "version": "2.2.2",
  "description": "__MSG_description__",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "http://*/*", "https://*/*", "contextMenus", "tabs"
  ],
  "minimum_chrome_version": "6",
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_title": "AdBlock Options",
    "popup": "button/popup.html"
  },
  "background_page": "background.html",
  "options_page": "options/index.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "port.js",
        "jquery/jquery-1.4.2.min.js", 
        "functions.js", 
        "broadcast_channel.js", 

      
        "filtering/filteroptions.js",
        "filtering/fifocache.js",
        "filtering/filtertypes.js",
        "filtering/filterset.js",


        "adblock_start.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "blacklisting/overlay.js", 
        "blacklisting/clickwatcher.js",
        "blacklisting/elementchain.js",
        "blacklisting/blacklistui.js",
        "blacklister.js", 
        "whitelister.js", 
        "adblock.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "default_locale": "en"
}
