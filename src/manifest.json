{
  "name": "AdBlock",
  "version": "2.5.42",
  "background": {
    "scripts": [
      "jquery/jquery.min.js",
      "port.js",
      "functions.js",
      "stats.js",
      "filtering/filteroptions.js",
      "filtering/fifocache.js",
      "filtering/filtertypes.js",
      "filtering/filterset.js",
      "filtering/myfilters.js",
      "filtering/filternormalizer.js",
      "idlehandler.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_popup": "button/popup.html",
    "default_title": "__MSG_optionstitle__"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "port.js",
        "functions.js", 
        "filtering/filteroptions.js",
        "adblock_start_common.js",
        "adblock_start_chrome.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "*://*.mail.live.com/*",
        "*://*.hk-pub.com/forum/thread-*",
        "*://*.mastertoons.com/*"
      ],
      "js": [
        "bandaids.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "uiscripts/blacklisting/rightclick_hook.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "content_security_policy": "default-src 'self'; style-src 'self' 'unsafe-inline'; connect-src *; frame-src 'self' https://chromeadblock.com",
  "default_locale": "en",
  "description": "__MSG_description1__",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "options_page": "options/index.html",
  "permissions": [
    "http://*/*", "https://*/*", "contextMenus", "tabs", "idle", "webRequest", "webRequestBlocking", "webNavigation"
  ],
  "web_accessible_resources": [
    "img/icon24.png",
    "jquery/css/custom-theme/images/ui-bg_inset-hard_100_fcfdfd_1x100.png",
    "jquery/css/custom-theme/images/ui-icons_056b93_256x240.png",
    "jquery/css/custom-theme/images/ui-icons_d8e7f3_256x240.png",
    "jquery/css/custom-theme/jquery-ui.custom.css",
    "jquery/css/override-page.css"
  ]
}
