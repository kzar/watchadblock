{
  "name": "AdBlock",
  "version": "2.4.2",
  "description": "__MSG_app_description1__",
  "icons": {
    "16": "img/icon16.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "http://*/*", "https://*/*", "contextMenus", "tabs", "notifications"
  ],
  "minimum_chrome_version": "6",
  "background_page": "background.html",
  "options_page": "options/index.html",
  "incognito": "spanning",
  "app": {
    "launch": {
      "local_path": "options/index.html"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "port.js",
        "jquery/jquery.min.js", 
        "functions.js", 
      
        "filtering/filteroptions.js",
        "filtering/filtertypes.js",
        "filtering/filterset.js",


        "adblock_start.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "*://*.mail.live.com/*",
        "*://*.hk-pub.com/forum/thread-*",
        "*://*.youtube.com/*"
      ],
      "js": [
        "bandaids.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "adblock.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "uiscripts/blacklisting/rightclick_hook.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "default_locale": "en"
}
