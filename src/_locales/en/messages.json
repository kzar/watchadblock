{
  //manifest.json
  "description": {
    "message": "AdBlock for Chrome!  Blocks ads all over the web.  Rated 4.5 out of 5 stars by our users!",
    "description": "Extension description in manifest"
  },

  //multiple scripts
  "timelog": {//adblock.js and adblock_start.js
    "message": "$scriptname$ run time: $runtime$ ms || $url$",
    "description": "Debug time log message to tell you how long a part of the code ran",
    "placeholders": {
      "scriptname": {
        "content": "$1",
        "example": "adblock_main"
      },
      "runtime": {
        "content": "$2",
        "example": "22"
      },
      "url": {
        "content": "$3",
        "example": "http://foo.com/"
      }
    }
  },
  "buttoncancel": {//blacklistui.js, clickwatcher.js, whitelister.js
    "message": "Cancel",
    "description": "Cancel button"
  },

  //adblock.js
  "selectorhid": {
    "message": "Debug: CSS '$selector$' hid:",
    "description": "Debug log message to tell you a selector has been matched",
    "placeholders": {
      "selector": {
        "content": "$1",
        "example": "div#adblock"
      }
    }
  },
  "purginglog": {
    "message": "Purging $node$: $url$",
    "description": "Debug log message to tell you an element has been purged",
    "placeholders": {
      "node": {
        "content": "$1",
        "example": "DIV"
      },
      "url": {
        "content": "$2",
        "example": "http://foo.com/"
      }
    }
  },
  "youtubeblockedlog": {
    "message": "Blocking YouTube ads",
    "description": "Debug log message to tell you ads have been removed from a YouTube video"
  },
  "youtubevideomessage": {
    "message": "No video? Reload the page. If this happens a lot, disable YouTube ad blocking under $link$.",
    "description": "Message on the YouTube video page that tells you that it might be the option that blocks the video",
    "placeholders": {
      "link": {
        "content": "$1AdBlock Options$2"
      }
    }
  },
  "adblockingpagelog": {
    "message": "==== ADBLOCKING PAGE: $url$",
    "description": "Debug log message that tells you a page is being adblocked",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://foo.com/"
      }
    }
  },
  "sweepingpagelog": {
    "message": "Sweeping the page because a new node was inserted.",
    "description": "Debug log message to tell you AdBlock ran again"
  },

  //adblock_start.js
  "chrometrueblock": {
    "message": "CHROME TRUE BLOCK: $url$",
    "description": "Debug log message that tells you a resource has been blocked",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://foo.com/"
      }
    }
  },

  //blacklister.js
  "logshowing": {
    "message": "showing",
    "description": "Debug log message to tell you an element is shown again"
  },

  //blacklistui.js
  "sliderexplanation": {
    "message": "Slide the slider until the ad is blocked correctly on the page, and the blocked element looks useful.",
    "description": "Blacklister slider page message"
  },
  "slidertitle": {
    "message": "Step 1: Figure out what to block",
    "description": "Blacklister slider page title"
  },
  "blacklisteroptions1": {
    "message": "What do you think will be true about this ad every time you visit this page?",
    "description": "Blacklister attribute chosing page message"
  },
  "blacklisternotsure": {
    "message": "$1Not sure?$2 just press 'Block it!' below.",
    "description": "Blacklister attribute chosing page message"
  },
  "blacklisterwanttohelp": {
    "message": "$1Want to help?$2 $reportad$",
    "description": "Blacklister attribute chosing page message",
    "placeholders": {
      "reportad": {
        "content": "$3Report the ad$4 so everybody can benefit!"
      }
    }
  },
  "blacklisterthefilter": {
    "message": "The filter, which can be changed on the Options page:",
    "description": "Blacklister attribute chosing page message"
  },
  "blacklisteroptionstitle": {
    "message": "Last step: What makes this an ad?",
    "description": "Blacklister options page title"
  },
  "blacklisternofilter": {
    "message": "No filter specified!",
    "description": "Warning if the user hasn't specified a filter"
  },
  "blacklistereditfilter": {
    "message": "Please type the correct filter below and press OK",
    "description": "Text if the user wants to manually edit a filter"
  },
  "blacklistereditinvalid": {
    "message": "The filter is invalid!",
    "description": "Messagebox if the manually edited filter is invalid"
  },
  "blacklisterblockedelement": {
    "message": "Blocked element:",
    "description": "Tells the user what the blocked element is on the slider page"
  },
  "blacklisterwarningnofilter": {
    "message": "Warning: no filter specified!",
    "description": "Warns the user that no filter was specified"
  },
  "blacklisterblocksalloftype": {
    "message": "Be careful: this filter blocks all $elementtype$ elements on the page!",
    "description": "Warns the user that all elements of type X will be blocked",
    "placeholders": {
      "elementtype": {
        "content": "$1",
        "example": "DIV"
      }
    }
  },
  "blacklistersinglematch": {
    "message": "That matches$1 1 $2item on this page.",
    "description": "Tells the user only one match was found"
  },
  "blacklistermatches": {
    "message": "That matches $matchcount$ items on this page.",
    "description": "Tells the user multiple matches were found",
    "placeholders": {
      "matchcount": {
        "content": "$1",
        "example": "5"
      }
    }
  },
  "blacklisterattrwillbe": {
    "message": "$attribute$ will be $value$",
    "description": "Checkbox label",
    "placeholders": {
      "attribute": {
        "content": "$1",
        "example": "class"
      },
      "value": {
        "content": "$2",
        "example": "AdBanner"
      }
    }
  },
  "blacklistertype": {
    "message": "Type",
    "description": "node name is called 'Type'"
  },
  "buttonlooksgood": {
    "message": "Looks good",
    "description": "Looks good button"
  },
  "buttonblockit": {
    "message": "Block it!",
    "description": "Block button"
  },
  "buttonedit": {
    "message": "Edit",
    "description": "Edit filter manually button"
  },
  "buttonback": {
    "message": "Back",
    "description": "Back to previous wizard page button"
  },

  //broadcast_channel.js
  "broadcastnodispatch": {
    "message": "No dispatch function '$requestfn$'",
    "description": "Error log message",
    "placeholders": {
      "requestfn": {
        "content": "$1",
        "example": "top_open_blacklist_ui"
      }
    }
  },
  
  //clickwatcher.js
  "findingads": {
    "message": "Finding ads...",
    "description": "When you press ctrl-shift-K the blacklister searches for ads"
  },
  "findingadswait": {
    "message": "This'll only take a moment.",
    "description": "When you press ctrl-shift-K the blacklister searches for ads"
  },
  "blockanadtitle": {
    "message": "Block an ad",
    "description": "Title of the first two dialogs of the blacklister"
  },
  "clickthead": {
    "message": "Click the ad, and I'll walk you through blocking it.",
    "description": "When you have to click the ad to continue"
  },

  //whitelister.js
  "whitelistertext": {
    "message": "AdBlock won't run on domains ending in",
    "description": "The message of the whitelister dialog"
  },
  "whitelistertitle": {
    "message": "Exclude this domain?",
    "description": "The title of the whitelister dialog"
  },
  "buttonok": {
    "message": "OK!",
    "description": "OK button"
  },
  
  //filtering/filterset.js
  "logmatched": {
    "message": "Matched: '$rule$' -> $url$",
    "description": "Debug log message to tell you a match has been found",
    "placeholders": {
      "rule": {
        "content": "$1",
        "example": "/show_ads.js"
      },
      "url": {
        "content": "$2",
        "example": "http://foo.com/"
      }
    }
  },
  "logwhitelisted": {
    "message": "Whitelisted: '$rule$' -> $url$",
    "description": "Debug log message to tell you a whitelisted resource has been found",
    "placeholders": {
      "rule": {
        "content": "$1",
        "example": "/show_ads.js"
      },
      "url": {
        "content": "$2",
        "example": "http://foo.com/"
      }
    }
  },
  
  //filtering/filtertypes.js
  "logunparsableselector": {
    "message": "Found an unparseable selector '$selector$'",
    "description": "Error log message",
    "placeholders": {
      "selector": {
        "content": "$1",
        "example": "div[id^$='a+b]"
      }
    }
  },
  "logregexfound": {
    "message": "Found a true regex rule - $regex$",
    "description": "Debug log message to tell you a regex has been found",
    "placeholders": {
      "regex": {
        "content": "$1",
        "example": "any regex"
      }
    }
  },
  "logregexunparsable": {
    "message": "Found an unparseable regex rule - $regex$",
    "description": "Debug log message to tell you a regex could not be parsed",
    "placeholders": {
      "regex": {
        "content": "$1",
        "example": "any broken regex"
      }
    }
  },
  
  //filtering/myfilters.js
  "logfetched": {
    "message": "Fetched $url$",
    "description": "Debug log message to tell you a list has been fetched",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://foo.com/list.txt"
      }
    }
  },
  "logfetchfailed": {
    "message": "Error fetching $url$",
    "description": "Debug log message to tell you a list could not be fetched",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://foo.com/list.txt"
      }
    }
  },
  
  //background.html
  "couldntparsejson": {
    "message": "Couldn't parse json for $key$",
    "description": "Error log message telling you an option could not be loaded",
    "placeholders": {
      "key": {
        "content": "$1",
        "example": "whitelist"
      }
    }
  },
  "timetorun": {
    "message": "time to run: $runtime$ ms || $domain$",
    "description": "Debug log message to tell you how long a part of the code ran",
    "placeholders": {
      "runtime": {
        "content": "$1",
        "example": "22"
      },
      "domain": {
        "content": "$2",
        "example": "foo.com"
      }
    }
  },
  "broadcasttousers": {
    "message": "Broadcasting from background to users $requestfn$",
    "description": "Debug log message",
    "placeholders": {
      "requestfn": {
        "content": "$1",
        "example": "top_open_blacklist_ui"
      }
    }
  },
  "broadcasttouserstab": {
    "message": "Broadcasting from background (to page $tabid$) to users $requestfn$",
    "description": "Debug log message",
    "placeholders": {
      "tabid": {
        "content": "$1",
        "example": "3221"
      },
      "requestfn": {
        "content": "$2",
        "example": "top_open_blacklist_ui"
      }
    }
  },
  "removedisconnectedport": {
    "message": "REMOVING DISCONNECTED PORT $port$",
    "description": "Debug log message",
    "placeholders": {
      "port": {
        "content": "$1",
        "example": "2:port"
      }
    }
  },
  "lenghtofports": {
    "message": "Length of ports: $length$",
    "description": "Debug log message",
    "placeholders": {
      "length": {
        "content": "$1",
        "example": "8"
      }
    }
  },
  "stupidport": {
    "message": "Removing stupid Options port which doesn't know to die",
    "description": "Debug log message"
  },
  "addingoptionsport": {
    "message": "Adding Options port for the first time.",
    "description": "Debug log message"
  },
  "runningfn": {
    "message": "Running: $requestfn$",
    "description": "Debug log message",
    "placeholders": {
      "requestfn": {
        "content": "$1",
        "example": "top_open_blacklist_ui"
      }
    }
  },
  "response": {
    "message": "Response:",
    "description": "Debug log message"
  },
  "undefined": {
    "message": "undefined",
    "description": "Debug log message"
  },
  "safaritrueblock": {
    "message": "SAFARI TRUE BLOCK $url$: $matched$",
    "description": "Debug log message that tells you a resource has been blocked",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://foo.com/"
      },
      "matched": {
        "content": "$2",
        "example": "true"
      }
    }
  },

  //adreport.html
  "adreportintro": {
    "message": "So you just found an ad on a web page? Well, I'll help you to find the correct place to report it!",
    "description": "Adreport intro message"
  },
  "willnotreportblacklister": {
    "message": "Remember: reporting ads is voluntary. If you don't wish to do this you can just close this window and block it only for yourself by clicking 'block it' in the blacklister wizard page!",
    "description": "Adreport intro message"
  },
  "willnotreportoptions": {
    "message": "Remember: reporting ads is voluntary. If you don't wish to do this you can just close this window and block it only for yourself by adding it's filter in the 'Blacklister' tab of the options page!",
    "description": "Adreport intro message"
  },
  "uptodatefilters": {
    "message": "Up to date filters",
    "description": "Adreport step 1"
  },
  "doesitstillappear": {
    "message": "If you update your filters and then reload the page that contained the ad, does the ad still appear?",
    "description": "Adreport step 1"
  },
  "clickupdatefiltersbutton": {
    "message": "You can update your filters by clicking this button:",
    "description": "Adreport step 1"
  },
  //<<<TO BE CONTINUED>>> but first wait until branches/issue-3106 is merged
  
  //subscribe.html
  "subscribingtitle": {
    "message": "Subscribing to filter list...",
    "description": "abp: link subscriber title"
  },
  "subscribingfinished": {
    "message": "Finished!",
    "description": "abp: link subscriber result"
  },
  "subscribingfailed": {
    "message": "Failed!",
    "description": "abp: link subscriber result"
  }
}
